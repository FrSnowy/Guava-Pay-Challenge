{
	"env": {
		"browser": true,
		"es2021": true 
	},
	"extends": [ 
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"react", 
		"prettier",
		"@typescript-eslint"
	],
	"rules": {
		"react/prop-types": 0,
		"@typescript-eslint/no-unused-vars": [
  		"error", // or error
  		{ 
    		"argsIgnorePattern": "^_+",
    		"varsIgnorePattern": "^_+",
    		"caughtErrorsIgnorePattern": "^_+"
  		}
		]
	}
}